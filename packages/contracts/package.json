{
  "name": "contracts",
  "version": "0.0.0",
  "private": true,
  "license": "MIT",
  "scripts": {
    "build": "forge clean && forge build",
    "tokengen": "tsx ./src/cli/tokengenScript.ts tokengen",
    "initialize": "pnpm run tablegen && pnpm run worldgen && pnpm run build && pnpm run worldtypes",
    "lint": "pnpm run prettier && pnpm run solhint",
    "prettier": "prettier --write 'src/**/*.sol'",
    "solhint": "solhint --config ./.solhint.json 'src/**/*.sol' --fix",
    "tablegen": "mud tablegen",
    "test": "mud test",
    "worldgen": "mud worldgen",
    "worldtypes": "rimraf types && typechain --target=ethers-v5 out/IWorld.sol/IWorld.json"
  },
  "dependencies": {
    "@ethersproject/abi": "^5.7.0",
    "@ethersproject/bytes": "^5.7.0",
    "@ethersproject/providers": "^5.7.2",
    "@latticexyz/cli": "2.0.0-alpha.1.197",
    "@latticexyz/common": "2.0.0-alpha.1.197",
    "@latticexyz/config": "2.0.0-alpha.1.197",
    "@latticexyz/schema-type": "2.0.0-alpha.1.197",
    "@latticexyz/std-contracts": "2.0.0-alpha.1.197",
    "@latticexyz/store": "2.0.0-alpha.1.197",
    "@latticexyz/world": "2.0.0-alpha.1.197",
    "@types/yargs": "^17.0.24",
    "ethers": "^5.7.2",
    "tsx": "^3.12.7",
    "yargs": "^17.7.2",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "@typechain/ethers-v5": "^10.2.0",
    "@types/node": "^18.15.11",
    "ds-test": "https://github.com/dapphub/ds-test.git#c9ce3f25bde29fc5eb9901842bf02850dfd2d084",
    "forge-std": "https://github.com/foundry-rs/forge-std.git#b4f121555729b3afb3c5ffccb62ff4b6e2818fd3",
    "prettier": "^2.6.2",
    "prettier-plugin-solidity": "^1.0.0-beta.19",
    "solhint": "^3.3.7",
    "typechain": "^8.1.1"
  }
}
